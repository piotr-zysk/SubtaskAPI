<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   namespace="SubtaskAPI.Models" assembly="SubtaskAPI">

  <class name="TaskOrder" table="TaskOrder">

    <id name="Id">
      <column name="Id" sql-type="int" not-null="true"/>
      <generator class="increment"/>
    </id>

    <property name="Ids">
      <column name="Ids" length="100" not-null="true" sql-type="nvarchar(255)" />
    </property>
    
  </class>

</hibernate-mapping>